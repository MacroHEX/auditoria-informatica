{
  "name": "backend-tickets",
  "version": "1.0.0",
  "description": "Backend del sistema de agendamiento de tickets",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.7",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "tickets",
    "socket.io",
    "auditoria",
    "banco"
  ]
}